'$Dynamic
Screen _NewImage(640, 480, 32)
Type LoadedWord
    X As Integer
    Y As Integer
    Word As String
    Colour As Long
    Speed As Integer
End Type
Start:
Clear
Randomize Timer
Dim Shared Words$(1 To 3000)
Dim Shared MaxWords
Dim Shared LoadedWords(1 To 50) As LoadedWord
For I = 1 To 3000
    Read Words$(I)
Next I
Dim Shared Score As _Integer64, Money As _Integer64
Money = 100
Do
    Cls
    _Limit 60
    While _MouseInput: Wend
    Line (240, 96 * (_MouseY \ 96) + 30)-(400, 96 * (_MouseY \ 96) + 70), _RGB32(0, 127, 255), BF
    For I = 1 To 5
        If _MouseY \ 96 = I - 1 Then Color _RGB32(0, 255, 0), _RGB32(0, 127, 255) Else Color _RGB32(255), _RGB32(0)
        Select Case I
            Case 1: _PrintString (304, 40), "Easy"
            Case 2: _PrintString (296, 136), "Medium"
            Case 3: _PrintString (304, 232), "Hard"
            Case 4: _PrintString (292, 328), "Extreme"
            Case 5: _PrintString (280, 424), "Impossible"
        End Select
    Next I
    Color _RGB32(255), _RGB32(0)
    If _MouseButton(1) Then
        Select Case _MouseY \ 96
            Case 0: WordDelayTime = 4
                DifficultyCounter = 1
                MODE$ = "Easy"
            Case 1: WordDelayTime = 3
                DifficultyCounter = 2
                MODE$ = "Medium"
            Case 2: WordDelayTime = 2
                DifficultyCounter = 3
                MODE$ = "Hard"
            Case 3: WordDelayTime = 1
                DifficultyCounter = 4
                MODE$ = "Extreme"
            Case 4: WordDelayTime = 0.5
                DifficultyCounter = 5
                MODE$ = "Impossible"
        End Select
        Exit Do
    End If
    _Display
Loop
MaxWords = 50 - WordDelayTime * 10 - 5 * (WordDelayTime = 0.5)
KeyboardListenerTimer = _FreeTimer
On Timer(KeyboardListenerTimer, 0.001) GoSub KeyboardListener
Timer(KeyboardListenerTimer) On
NewWordTimer = _FreeTimer
On Timer(NewWordTimer, WordDelayTime) GoSub NewWord
Timer(NewWordTimer) On
GoSub NewWord
Color _RGB32(255), _RGB32(15)
LastWordTimer = Timer(0.1)
FPS = 10
Do
    Cls , _RGB32(15)
    _Limit FPS
    _KeyClear
    If _WindowHasFocus = 0 Then Paused = -1
    If Paused = 0 Then
        ClearInputWord = 0
        For I = 1 To MaxWords
            If Len(LoadedWords(I).Word) Then
                If PowerUp_Freeze = 0 Then LoadedWords(I).X = LoadedWords(I).X + LoadedWords(I).Speed / FPS
                Color LoadedWords(I).Colour, _RGB32(15)
                _PrintString (LoadedWords(I).X, LoadedWords(I).Y), LoadedWords(I).Word
                If LoadedWords(I).X + Len(LoadedWords(I).Word) * _FontWidth > 640 Then _PrintString (0, 16), "You Lose": _AutoDisplay: Exit Do
                If InStr(InputWord$, LoadedWords(I).Word) Then
                    Score = Score + Len(LoadedWords(I).Word) * (10 - Timer(0.1) + LastWordTimer) - (Len(InputWord$) - Len(LoadedWords(I).Word)) + LoadedWords(I).Speed + (_Width - LoadedWords(I).X - Len(LoadedWords(I).Word) * _FontWidth) * LoadedWords(I).Speed / _Width
                    Money = Money + Len(LoadedWords(I).Word) * (9 - WordDelayTime) / 4
                    LastWordTimer = Timer(0.1)
                    LoadedWords(I).Word = ""
                    ClearInputWord = -1
                    PauseTimer = 0
                End If
            End If
        Next I
        If ClearInputWord Then InputWord$ = ""
        If PowerUp_Freeze Then PowerUp_Freeze = PowerUp_Freeze - Sgn(PowerUp_Freeze)
    Else
        _AutoDisplay
        Line (_Width / 2 - 4 * _FontWidth, _Height / 2 - 3 * _FontHeight / 2)-(_Width / 2 + 4 * _FontWidth, _Height / 2 + 3 * _FontHeight / 2), _RGB32(127), BF
        Color _RGB32(255), _RGB32(127): _PrintString (_Width / 2 - 3 * _FontWidth, _Height / 2 - _FontHeight / 2), "Paused"
        PauseTimer = Timer(0.1)
        Timer(KeyboardListenerTimer) Off
        Timer(NewWordTimer) Off
        'Do While _KeyDown(100306) and (_KeyDown(80) Or _KeyDown(112)): Loop
        Sleep
        Timer(KeyboardListenerTimer) On
        Timer(NewWordTimer) On
        LastWordTimer = Timer(0.1) - PauseTimer + LastWordTimer
        Paused = 0
    End If
    If InputWord$ = "exit" + Chr$(13) Then System
    Color _RGB32(255), _RGB32(15)
    _PrintString (0, 464), "Word: " + InputWord$
    _PrintString (0, 0), "Score: " + _Trim$(Str$(Score))
    _PrintString (592 - 8 * Len(MODE$), 0), "Mode: " + MODE$
    _PrintString (320, 0), "Money: " + _Trim$(Str$(Money))
    _Display
Loop
Timer(KeyboardListenerTimer) Free
Timer(NewWordTimer) Free
Do
    If InKey$ = Chr$(13) Then Exit Do
Loop
GoTo Start
System
KeyboardListener:
If (_KeyDown(100306) = -1 And (_KeyDown(80) = -1 Or _KeyDown(112) = -1)) Then Paused = -1: Return
Key$ = InKey$
If Key$ = Chr$(8) Then
    If Money > 5 Then
        InputWord$ = Left$(InputWord$, Len(InputWord$) - 1)
        Money = Money - 5
    End If
ElseIf Key$ = "[" Then
    If Money > 500 * DifficultyCounter Then
        For I = 1 To 4 * (WordDelayTime + 1)
            LoadedWords(GetMax_OffsetX).Word = ""
        Next I
        Money = Money - 500
        Score = Score + I + 500
    End If
ElseIf Key$ = "]" Then
    If Money > 250 * DifficultyCounter Then
        PowerUp_Freeze = FPS * 5
        Money = Money - 250
        Score = Score + 250
    End If
ElseIf Key$ = "\" Then
    If Money > 100 * DifficultyCounter Then
        W$ = LoadedWords(GetMin_Length).Word
        For I = 1 To 3 * (WordDelayTime + 1) + 1
            LoadedWords(GetMax_OffsetX).Word = W$
        Next I
        Money = Money - 100
        Score = Score + 100
    End If
Else
    If Len(Key$) = 1 Then
        If Asc(Key$) = 13 Or (Asc(Key$) >= 65 And Asc(Key$) <= 122) Or Asc(Key$) = 45 Then InputWord$ = InputWord$ + Key$
    End If
End If
Return
NewWord:
ID = Int(Rnd * 3000) + 1
LW = 0
For I = 1 To MaxWords
    If Len(LoadedWords(I).Word) = 0 Then LW = I: Exit For
Next I
If LW > 0 Then
    LoadedWords(LW).X = 1
    LoadedWords(LW).Y = Int(Rnd * (_Height - 32) / 16) * 16 + 16
    LoadedWords(LW).Word = LCase$(Words$(ID))
    LoadedWords(LW).Colour = _RGB32(128 + Rnd * 128, 128 + Rnd * 128, 128 + Rnd * 128)
    If Len(LoadedWords(LW).Word) <= 5 And Rnd < 0.05 Then LoadedWords(LW).Speed = 20 Else LoadedWords(LW).Speed = 10
End If
Return

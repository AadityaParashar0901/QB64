Function GetMax_OffsetX ()
    m = 1
    For I = 1 To MaxWords
        If Len(LoadedWords(I).Word) Then If LoadedWords(I).X + Len(LoadedWords(I).Word) * _FontWidth > LoadedWords(m).X + Len(LoadedWords(m).Word) * _FontWidth Then m = I
    Next I
    GetMax_OffsetX = m
End Function
Function GetMin_Length ()
    m = 1
    For I = 1 To MaxWords
        If InRange(0, Len(LoadedWords(I).Word), Len(LoadedWords(m).Word)) Then m = I
    Next I
    GetMin_Length = m
End Function
Function InRange (A, B, C)
    If A < B And B < C Then InRange = -1
End Function
